    "start": "arc sandbox"
  },
  "dependencies": {
    "@remix-run/architect": "^1.2.3",
    "@remix-run/react": "^1.2.3",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
